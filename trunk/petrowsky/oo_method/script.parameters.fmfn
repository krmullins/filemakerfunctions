/*
=====================================================
script.parameters( style )

RETURNS:    (mixed) structured fomat of script parameters.
DEPS:       string.explode()
NOTES:
=====================================================
*/

Case(

style = "single";   Get( ScriptParameter );
style = "piped";    string.explode ( Get( ScriptParameter ) ; "|" );
style = "named";    "Not supported yet";
style = "xml";      "Not supported yet";
0

)